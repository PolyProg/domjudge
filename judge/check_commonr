#/usr/bin/env bash

CHECKER_BINARY=polynomial_checker
CHECKER_DIR=/opt/domjudge/judgehost/lib/judge/
OUTPUT=$($CHECKER_DIR/$CHECKER_BINARY "$@" 2>&1)
RETURN_VAL=$?

echo "Returning $RETURN_VAL"
if [ $RETURN_VAL == 0 ]; then
  echo "Exit OK"
  exit 0
fi

echo "Exit NOT OK"
echo $OUTPUT

if [ $RETURN_VAL == 3 ]; then
  exit 17
fi

exit 0
