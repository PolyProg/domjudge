#!/bin/bash

CHECKER_BINARY=polynomial_checker
CHECKER_DIR=/opt/domjudge/judgehost/lib/judge/
OUTPUT=$($CHECKER_DIR/$CHECKER_BINARY "$@" 2>&1)
RETURN_VAL=$?

echo "Returning x$RETURN_VAL"
if [ "x$RETURN_VAL" == "x0" ]; then
  echo "Exit OK"
  exit 0
fi

echo "Exit NOT OK"
echo $OUTPUT

if [ "x$RETURN_VAL" == "x3" ]; then
  exit 17
fi

exit 0
